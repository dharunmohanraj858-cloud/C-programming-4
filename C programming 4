int next_permutation(int n, char **s) {
    int i = n - 2;
    while (i >= 0 && strcmp(s[i], s[i + 1]) >= 0)
        i--;

    if (i < 0) return 0;

    int j = n - 1;
    while (strcmp(s[i], s[j]) >= 0)
        j--;

    char *temp = s[i];
    s[i] = s[j];
    s[j] = temp;

    for (int l = i + 1, r = n - 1; l < r; l++, r--) {
        temp = s[l];
        s[l] = s[r];
        s[r] = temp;
    }
    return 1;
}
